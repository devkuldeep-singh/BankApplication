<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Register here</title>


<style type="text/css">

body{
margin: 0;
  padding: 0;
background-color: #E6F4FF;}
.content{
display:flex;
justify-content: center;

}

header {
    display: flex;
    justify-content: space-between;/* space between heading & links */
    align-items: center; /* vertically center */
   padding: 0px 20px;
    background-color: #004AAD;
    color:#FFFFFF;
 border-radius: 12px 12px 0px 0px;

}

header div a {
    margin-right: 15px;
    text-decoration: none;
    font-size: 18px;
    color: #FFA500; 
}

#img{
width: 30px;
height: 30px;
border-radius: 50%;
}

.div{
width:500px;
height:100%;
background-color:window;
border: none;
border-radius: 15px;
padding-bottom: 40px;
padding-left: 30px;
padding-right: 30px;
}

#fname, #email, #phone,#uname,#cpass,#pass,input[type="submit"] {
box-sizing:border-box;
width: 100%;
height: 35px;
padding-left: 7px;
border: 1px solid gray;
border-radius: 5px;
margin-top: 5px;
}
.child{
padding-left: 20px;
padding-right: 20px;
}
input[type="submit"] {
background-color: blue;
}
#top {
    text-align: center;
}

#top h1{
color: blue;
}

#top h5{
color:gray;
}

#footer1{
background-color:#004AAD;
color:window;
text-align: center; 
padding: 10px 0px;
border-radius:0 0 15px 15px;

}
a{
text-decoration: none;
}

#p1{
text-align: center;
}

.msg{
text-align: center;
color: red;
font-size: 12px;
margin: 0;
padding: 0;
margin-bottom: 5px;
}

</style>
</head>
<body>

<header><h1><img alt="img" src="data:image/jpeg;base64," id="img">
Bharat Sarkar Bank</h1><br>
<div>
<a href="Login.jsp">Login Page</a>
<a href="Support.html">Customer Support</a>
</div>
</header>





<h3 class="content">
<div class="div"> 
<div class="child">
<div id="top">
    <h1>Create your BSB Account</h1>
    <h5>Join our BSB banking platform today</h5>
</div>

<form action="register" method="post">
<label for="fname">Full Name</label><br>
<input type="text" name="fname"  id="fname" placeholder="Enter your fullname" required="required"><br><br>

<label for="email">Eamil Address</label><br>
<input type="text" name="email"  id="email" placeholder="Enter your email" required="required"><br><br>

<label for="phone">Phone number</label><br>
<input type="text" name="phone"  id="phone" placeholder="Enter your phone number" required="required"><br><br>

<label for="uname">User Name</label><br>
<input type="text" name="uname"  id="uname" placeholder="Choose a username" required="required"><br><br>

<label for="pass">Password</label><br>

<div style="position: relative; width:100%;">
    <input type="password" 
           name="pass"  
           id="pass" 
           placeholder="Create password" 
           required="required"
           style="width: 100%; padding-right: 35px;">
    
    <span id="togglePass"
          style="position: absolute; right: 10px; top: 10px; cursor: pointer; font-size: 18px;">
        üëÅÔ∏è
    </span>
</div>

<br><br>

<label for="cpass">Confirm Password</label><br>
<input type="text" name="cpass"  id="cpass" placeholder="Confirm password" required="required"><br><br>
<div >
<% String msg=(String)request.getAttribute("msg1");

if(msg!=null){
	%>
<p class="msg"><%=msg%></p>
<%} %>
</div>
<label>
    <input type="checkbox" name="agree" required>
    I agree to the <a href="">Terms & Conditions</a>
</label>

<input type="submit" value="Register" >
</form>
<p id="p1">Already have an account?<a href="Login.jsp">Login here</a></p>
</div>
</div>

</h3>

<footer id="footer1"><h2>&copy; 2025 Bharat Sarkar Bank All rights reserved</h2> </footer>

<script>
document.getElementById("togglePass").onclick = function () {
    var x = document.getElementById("pass");
    if (x.type === "password") {
        x.type = "text";
        this.textContent = "üö´";
    } else {
        x.type = "password";
        this.textContent = "üëÅÔ∏è";
    }
};
</script>
</body>
</html>